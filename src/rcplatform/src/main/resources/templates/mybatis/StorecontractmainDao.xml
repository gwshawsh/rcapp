<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ruanchuangsoft.platform.dao.StorecontractmainDao">

	<select id="queryObject" resultType="com.ruanchuangsoft.platform.entity.StorecontractmainEntity">
		select * from storecontractmain where id = #{value}
	</select>

	<select id="queryList" resultType="com.ruanchuangsoft.platform.entity.StorecontractmainEntity">
		select * from storecontractmain 
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from storecontractmain 
	</select>
	 
	<insert id="save" parameterType="com.ruanchuangsoft.platform.entity.StorecontractmainEntity" useGeneratedKeys="true" keyProperty="id">
		insert into storecontractmain
		(
			`billno`, 
			`orgid`, 
			`paytype`, 
			`boctid`, 
			`rate`, 
			`taxrate`, 
			`takeboxfee`, 
			`takeboxfeetax`, 
			`bgndate`, 
			`enddate`, 
			`remark`, 
			`billstatus`, 
			`makeuser`, 
			`makedate`, 
			`accuser`, 
			`accdate`, 
			`uptdate`
		)
		values
		(
			#{billno}, 
			#{orgid}, 
			#{paytype}, 
			#{boctid}, 
			#{rate}, 
			#{taxrate}, 
			#{takeboxfee}, 
			#{takeboxfeetax}, 
			#{bgndate}, 
			#{enddate}, 
			#{remark}, 
			#{billstatus}, 
			#{makeuser}, 
			#{makedate}, 
			#{accuser}, 
			#{accdate}, 
			#{uptdate}
		)
	</insert>
	 
	<update id="update" parameterType="com.ruanchuangsoft.platform.entity.StorecontractmainEntity">
		update storecontractmain 
		<set>
			<if test="billno != null">`billno` = #{billno}, </if>
			<if test="orgid != null">`orgid` = #{orgid}, </if>
			<if test="paytype != null">`paytype` = #{paytype}, </if>
			<if test="boctid != null">`boctid` = #{boctid}, </if>
			<if test="rate != null">`rate` = #{rate}, </if>
			<if test="taxrate != null">`taxrate` = #{taxrate}, </if>
			<if test="takeboxfee != null">`takeboxfee` = #{takeboxfee}, </if>
			<if test="takeboxfeetax != null">`takeboxfeetax` = #{takeboxfeetax}, </if>
			<if test="bgndate != null">`bgndate` = #{bgndate}, </if>
			<if test="enddate != null">`enddate` = #{enddate}, </if>
			<if test="remark != null">`remark` = #{remark}, </if>
			<if test="billstatus != null">`billstatus` = #{billstatus}, </if>
			<if test="makeuser != null">`makeuser` = #{makeuser}, </if>
			<if test="makedate != null">`makedate` = #{makedate}, </if>
			<if test="accuser != null">`accuser` = #{accuser}, </if>
			<if test="accdate != null">`accdate` = #{accdate}, </if>
			<if test="uptdate != null">`uptdate` = #{uptdate}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="delete">
		delete from storecontractmain where id = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from storecontractmain where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>