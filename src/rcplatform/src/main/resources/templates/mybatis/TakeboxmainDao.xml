			<if test="uptdate != null">`uptdate` = #{uptdate}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="delete">
		delete from takeboxmain where id = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from takeboxmain where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>