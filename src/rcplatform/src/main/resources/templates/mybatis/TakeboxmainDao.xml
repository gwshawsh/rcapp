			#{billno}, 
			#{refbillno}, 
			#{refbilltype}, 
			#{orgId}, 
			#{ladingcode}, 
			#{shipname}, 
			#{flight}, 
			#{portid}, 
			#{boxqty}, 
			#{boxtype}, 
			#{takeboxplaceid}, 
			#{endplaceid}, 
